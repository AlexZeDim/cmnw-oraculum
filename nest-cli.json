{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/cmnw-oraculum/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/cmnw-oraculum/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/cmnw-oraculum",
  "projects": {
    "rainy": {
      "type": "application",
      "root": "apps/rainy",
      "entryFile": "main",
      "sourceRoot": "apps/rainy/src",
      "compilerOptions": {
        "tsConfigPath": "apps/rainy/tsconfig.app.json"
      }
    },
    "pg": {
      "type": "library",
      "root": "libs/pg",
      "entryFile": "index",
      "sourceRoot": "libs/pg/src",
      "compilerOptions": {
        "tsConfigPath": "libs/pg/tsconfig.lib.json"
      }
    },
    "fefenya": {
      "type": "application",
      "root": "apps/fefenya",
      "entryFile": "main",
      "sourceRoot": "apps/fefenya/src",
      "compilerOptions": {
        "tsConfigPath": "apps/fefenya/tsconfig.app.json"
      }
    },
    "config": {
      "type": "library",
      "root": "libs/config",
      "entryFile": "index",
      "sourceRoot": "libs/config/src",
      "compilerOptions": {
        "tsConfigPath": "libs/config/tsconfig.lib.json"
      }
    },
    "pepa-chat-gpt": {
      "type": "application",
      "root": "apps/pepa-chat-gpt",
      "entryFile": "main",
      "sourceRoot": "apps/pepa-chat-gpt/src",
      "compilerOptions": {
        "tsConfigPath": "apps/pepa-chat-gpt/tsconfig.app.json"
      }
    },
    "rodriga": {
      "type": "application",
      "root": "apps/rodriga",
      "entryFile": "main",
      "sourceRoot": "apps/rodriga/src",
      "compilerOptions": {
        "tsConfigPath": "apps/rodriga/tsconfig.app.json"
      }
    },
    "mongo": {
      "type": "library",
      "root": "libs/mongo",
      "entryFile": "index",
      "sourceRoot": "libs/mongo/src",
      "compilerOptions": {
        "tsConfigPath": "libs/mongo/tsconfig.lib.json"
      }
    },
    "core": {
      "type": "library",
      "root": "libs/core",
      "entryFile": "index",
      "sourceRoot": "libs/core/src",
      "compilerOptions": {
        "tsConfigPath": "libs/core/tsconfig.lib.json"
      }
    }
  }
}
