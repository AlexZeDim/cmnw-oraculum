{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/cmnw-oraculum/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/cmnw-oraculum/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/cmnw-oraculum",
  "projects": {
    "rainy": {
      "type": "application",
      "root": "apps/rainy",
      "entryFile": "main",
      "sourceRoot": "apps/rainy/src",
      "compilerOptions": {
        "tsConfigPath": "apps/rainy/tsconfig.app.json"
      }
    },
    "pg": {
      "type": "library",
      "root": "libs/pg",
      "entryFile": "index",
      "sourceRoot": "libs/pg/src",
      "compilerOptions": {
        "tsConfigPath": "libs/pg/tsconfig.lib.json"
      }
    },
    "shared": {
      "type": "library",
      "root": "libs/shared",
      "entryFile": "index",
      "sourceRoot": "libs/shared/src",
      "compilerOptions": {
        "tsConfigPath": "libs/shared/tsconfig.lib.json"
      }
    },
    "fefenya": {
      "type": "application",
      "root": "apps/fefenya",
      "entryFile": "main",
      "sourceRoot": "apps/fefenya/src",
      "compilerOptions": {
        "tsConfigPath": "apps/fefenya/tsconfig.app.json"
      }
    }
  }
}