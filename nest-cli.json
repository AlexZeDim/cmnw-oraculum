{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/cmnw-oraculum/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
